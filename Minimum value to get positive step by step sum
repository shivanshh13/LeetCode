#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int minStartValue(int* nums, int numsSize) {
    int min_prefix_sum = INT_MAX;
    int current_sum = 0;

    for (int i = 0; i < numsSize; i++) {
        current_sum += nums[i];
        if (current_sum < min_prefix_sum) {
            min_prefix_sum = current_sum;
        }
    }

    if (min_prefix_sum < 1) {
        return 1 - min_prefix_sum;
    } else {
        return 1;
    }
}
